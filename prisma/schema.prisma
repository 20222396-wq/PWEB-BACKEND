generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"

}

model Usuario {
    id         Int       @id @default(autoincrement())
    username   String
    email      String    @unique
    contrase√±a String
    activo    Boolean   @default(true)
}

model Coins {
    id        Int     @id @default(autoincrement())
    nombre    String
    simbolo   String
    precio    Float
    marketCap Float
    volumen   Float
}